        "react-leaflet": ">=4.1.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "leaflet": ">=1.5.0",
        "prettier": "^1.16.4",
        "react": ">=18.2.0",
        "react-dom": ">=18.2.0",
        "react-leaflet": ">=4.1.0",
        "tslint": "^5.13.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.6.4",
        "uglify-js": "^3.5.15"
    },
    "dependencies": {
        "@types/leaflet": "^1.5.0"
    }
