        "typescript",
        "react-leaflet-v4"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lud-hu/leaflet-hotline-react/issues"
    },
    "homepage": "https://github.com/lud-hu/leaflet-hotline-react",
    "peerDependencies": {
        "leaflet": ">=1.9.1",
        "react": ">=18.2.0",
